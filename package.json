{
  "name": "pdf-annotator-ai-tutor",
  "private": true,
  "version": "1.0.0",
  "description": "An application to upload, markup PDFs with handwritten and text notes, convert handwriting to LaTeX, and get AI-powered tutoring hints based on your work.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "pdfjs-dist": "3.11.174",
    "katex": "^0.16.22",
    "react-katex": "^3.0.0",
    "@google/genai": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "jsdom": "^24.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "pdf",
    "annotation",
    "ai",
    "tutor",
    "latex",
    "handwriting",
    "gemini",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/pdf-annotator-ai-tutor.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pdf-annotator-ai-tutor/issues"
  },
  "homepage": "https://github.com/yourusername/pdf-annotator-ai-tutor#readme"
}
